#include <stdio.h>
#include<string.h>
#define tam = 100;


void sortStrings(char Arr[][100], int n);

void sortStrings(char Arr[][100], int n){ 
    char temp[100]; 
  
    //Sorting strings usando bubble sort 
    for (int j=0; j<n-1; j++) 
    { 
        for (int i=j+1; i<n; i++) 
        { 
            if (strcmp(Arr[j], Arr[i]) > 0) 
            { 
                strcpy(temp, Arr[j]); 
                strcpy(Arr[j], Arr[i]); 
                strcpy(Arr[i], temp); 
            } 
        } 
    } 
}


int main(){ 
       
  void sortStrings(char Arr[][100], int n); 
  
  return 0;
} 
